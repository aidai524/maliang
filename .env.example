NODE_ENV=development
PORT=3000

# Database
DATABASE_URL=postgresql://app:app@localhost:5432/imagesaas

# Redis
REDIS_URL=redis://localhost:6379

# Object Storage (Cloudflare R2) - Optional
R2_ACCOUNT_ID=your_account_id
R2_ACCESS_KEY_ID=your_access_key
R2_SECRET_ACCESS_KEY=your_secret_key
R2_BUCKET_NAME=images
R2_PUBLIC_BASE_URL=https://cdn.yourdomain.com/

# Local Storage Configuration
STORAGE_TYPE=local
PUBLIC_BASE_URL=http://localhost:3000

# Webhook signing (global)
WEBHOOK_SIGNING_SECRET=your_global_webhook_secret

# Gemini/Nano Banana API
# Note: Provider keys should be stored in database (provider_keys table)
# These are optional fallback/initial keys
GEMINI_API_KEY_1=your_gemini_api_key_1
GEMINI_API_KEY_2=your_gemini_api_key_2
GEMINI_API_BASE=https://generativelanguage.googleapis.com
GEMINI_MODEL=gemini-3.0-pro-vision

# Rate limits
GLOBAL_RPM_LIMIT=1000
GLOBAL_CONCURRENCY_LIMIT=200

# Worker concurrency (per process)
WORKER_CONCURRENCY=50

# Test API Key (optional - used by npm run init)
# If set, this key will be used for the test tenant instead of a random one
TEST_API_KEY=img_test_your_fixed_api_key_here
